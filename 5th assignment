    while 'AUG' in seq:
        seq=seq[seq.find('AUG'):] 
        for i in stops:
            for j in range(0,len(seq),3):
                if seq[j:j+3]==i:
                    orf=seq[:j]
                    protlst.append(orf.translate(to_stop=True))
        seq=seq[1:]
